<mvc:View controllerName="ap.be.materiaal.controller.App"
    xmlns:html="http://www.w3.org/1999/xhtml"
    xmlns:mvc="sap.ui.core.mvc" displayBlock="true"
    xmlns="sap.m" xmlns:f="sap.f"
	height="100%" >
    <f:FlexibleColumnLayout
        id="fcl"
        backgroundDesign="Solid"
    >
        <f:beginColumnPages>
            <mvc:XMLView
                id="beginView"
                viewName="ap.be.materiaal.view.main"
            />
        </f:beginColumnPages>
        <f:midColumnPages>
            <mvc:XMLView
                id="detailView"
                viewName="ap.be.materiaal.view.MaterialDetail"
            />
        </f:midColumnPages>
    </f:FlexibleColumnLayout>
</mvc:View>
